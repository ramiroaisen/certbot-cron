{
  "apps": [{
    "name": "certbot-hook",
    "script": "./cron.js",
    "args": "--renew-hook",
    "instances": 1,
    "exec_mode": "fork",
    "max_memory_restart": "200M",
    "log_date_format": "YYYY-MM-DD HH:mm:ss.SSS UTC-3",
    "merge_logs": true,
    "stop_exit_codes": 0,
    "kill_timeout": 5000,
    "env": {
      "NODE_ENV": "production"
    }
  }]
}